   "source": [
    "SELECT\n",
    "    SQRT(POWER(B.x - A.x, 2) + POWER(B.y - A.y, 2)) AS ShortestDistance\n",
    "FROM\n",
    "    Points A,\n",
    "    Points B\n",
    "WHERE\n",
    "    A.id = 1 AND B.id = 2; -- Replace 1 and 2 with the IDs of the points you're interested in\n"
   ]
